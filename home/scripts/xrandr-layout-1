#!/bin/sh
xrandr --output DP-0.8 --mode 2560x1440 --pos 0x1440 --rotate normal --output DVI-D-0 --off --output HDMI-0 --mode 2560x1440 --pos 5120x696 --rotate right --output DP-0 --off --output DP-1 --off --output DP-2 --mode 2560x1440 --pos 2560x0 --rotate normal --output DP-3 --off --output DP-4 --primary --mode 2560x1440 --pos 2560x1440 --rotate normal --rate 144 --output DP-5 --off --output DVI-D-1-0 --off --output HDMI-1-0 --off --output HDMI-1-1 --off --output DP-1-0 --off --output DP-1-1 --off --output DP-1-2 --off --output DP-1-3 --off
